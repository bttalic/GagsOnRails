<% if @feed_items.any? %>
<ol class="microposts">
	<%= render partial: 'shared/feed_item', collection: @feed_items %>
</ol>
<%= will_paginate @feed_items, :params=>{"type"=>"micro"} %>
<%else%>
<div class="alert alert-warning">
					Nothing in your feed, add some followers!
			</div>
<% end %>